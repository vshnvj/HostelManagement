@model HostelManagement.Models.User

@{
    ViewBag.Title = "TrackRent";
    Layout = "~/Views/Shared/User_layout.cshtml";
}



<div class="navbar fixed-top  navbar-light" style="background-color: #e3f2fd">
    <ul class="nav navbar-nav">
        <li>@Html.ActionLink("Profile", "Edit", new { id = Model.Id })</li>
        @*<li> @Html.ActionLink("Submit your application", "Requests", new { id = Model.Id })</li>*@
        <li> @Html.ActionLink("Status of appliation", "SeeRequests", new { id = Model.Id }) </li>
        <li>   @Html.ActionLink("complaint/feedback", "Feedback", "UsersHome", new { id = Model.Id })</li>

        <li>   @Html.ActionLink("track rent", "TrackRent", new { id = Model.Id })</li>

        <li>@Html.ActionLink("Logout", "Logout", "UsersHome")</li>

    </ul>
    </div>
<h2>TrackRent</h2>


<table class="table table-bordered">
    <tr>
        <th>
            Amount
        </th>
        <th>
            Date_of_payment
        </th>
        @*<th>
                @Html.DisplayNameFor(model => model.User.Name)
            </th>*@
        <th></th>
    </tr>

    @foreach (var item in ViewBag.List)
    {
        <tr>
            <td>
                @item.Amount
            </td>
            <td>
                @item.Date_of_payment.ToString("dd/MM/yyyy")
            </td>
            @*<td>
                    @Html.DisplayFor(modelItem => item.User.Name)
                </td>*@
        </tr>
    }

</table>
